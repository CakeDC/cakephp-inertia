{
    "name": "cakedc/cakephp-inertia",
    "description": "Inertia plugin for CakePHP",
    "type": "cakephp-plugin",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Inertia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Inertia\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
